{
    "manifest_version": 2,
    "name": "Eztrackr",
    "description": "Ease your job hunt by adding your jobs in an organized Trello board",
    "version": "3.0.0",
    "browser_action": {
        "default_icon": "icons/icon_128.png",
        "default_popup": "popup.html"
    },
    "icons": {
       "128": "icons/icon_128.png" 
    },
    "permissions": [
        "activeTab"
    ],
    "web_accessible_resources": [
        "settings/index.html"
    ]
}
